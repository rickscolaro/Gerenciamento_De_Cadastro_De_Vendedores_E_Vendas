@model IEnumerable<ProjetoSalesWebMvc.Models.Seller> /* O Objeto model é a classe que você quer retornar o valor e seu*/

@{
    ViewData["Title"] = "Sellers";
}

<h2>@ViewData["Title"]</h2>

<p><a asp-action="Create" class="btn btn-info">Create New</a></p>

<table class="table table-striped table-hover">
    <!--Cabeçalho-->
    <thead>
        <tr class="bg-success text-dark">
            <th>
                @Html.DisplayNameFor(model=> model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model=> model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model=> model.BirthDate)
            </th>
            <th>
                @Html.DisplayNameFor(model=> model.BaseSalary)
            </th>
            <th></th>
        </tr>
    </thead>
    <!--Corpo da tabela-->
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                @Html.DisplayFor(x=>item.Name)
                </td>
                <td>
                    @Html.DisplayFor(x=>item.Email)
                </td>
                <td>
                    @Html.DisplayFor(x=>item.BirthDate)
                </td>
                <td>
                    @Html.DisplayFor(x=>item.BaseSalary)
                </td>
                <td>
                    <!-- Já gera o gera o Link para o Id do contato que quer editar-->
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>  |

                    <a asp-action="Details" asp-route-id="@item.Id">Details</a>   |

                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a> 
                </td>
            </tr>
        }
    </tbody>
</table>